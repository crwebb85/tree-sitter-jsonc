{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "jsonc",
      "camelcase": "Jsonc",
      "title": "Jsonc",
      "scope": "source.jsonc",
      "path": "./",
      "file-types": ["jsonc"],
      "injection-regex": "^jsonc$",
      "class-name": "TreeSitterJsonc"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "jsonc grammar and parser for tree-sitter",
    "authors": [
      {
        "name": "Christopher Webb"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-jsonc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}

